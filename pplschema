user {
    id: string,
    username: string,
    email: string,
    password: string,
    universityId: int
    freelancer: string,
    description: [
        about: string,
        languages: string,
        skill: string,
        education: string,
        certification: string
    ],
}

university {
    id: string,
    name: string
}

job {
    id: string,
    name: string,
    employerId: string,
    freelancerId: string,
    deadline: date,
    fee: integer,
    completed: boolean,
    payment: [
        paymentDate: date,
        amount: integer,
    ],
    category: string,
}
